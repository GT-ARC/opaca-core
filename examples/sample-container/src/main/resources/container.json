{
  "imageName": "sample-agent-container-image",

  "requires": ["sample-requirement"],
  "provides": ["sample-provision"],

  "name": "Sample JIAC++ Agent Container",
  "version": "0.1.9",
  "description": "Just a sample container doing nothing but receiving and reacting to API calls",
  "provider": "DAI-Lab",

  "apiPort": 8082,
  "extraPorts": {
    "8888": {
      "protocol": "TCP",
      "description": "Simple Web Server for testing TCP port"
    },
    "8889": {
      "protocol": "UDP",
      "description": "Simple Web Server for testing UDP port"
    }
    
  },
  "definitions": {
    "car": {
      "inherits": "vehicle",
      "type": "car",
      "parameters": {
        "model": {
          "name": "model",
          "type": "String",
          "optional": "false"
        },
        "pessengers": {
          "name": "pessengers",
          "type": "Array",
          "optional": "false",
          "items": {
            "type": "String"
          }
        },
        "isFunctional": {
          "name": "isFunctional",
          "type": "Boolean",
          "optional": "false"
        },
        "construction year": {
          "name": "construction year",
          "type": "Int",
          "optional": "false"
        }
      }
    },
    "vehicle": {
      "type":"vehicle",
      "inherits": null,
      "parameters": {

        "num of wheels": {
          "name": "num of wheels",
          "type": "Number",
          "optional": "false"
        }
      }
    }
  }

}
